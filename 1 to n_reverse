#include <stdio.h>


int main() {
    int i,n;
    printf("enter a range : ");
    scanf("%d",&n);
    printf("Numbers from 1 to %d:\n",n);
    for (i = 1; i <= n; i++) {
        printf("%d ", i);
    }
    printf("\n");
    printf("Numbers from n to 1:\n",n);
    for (i = n; i >= 1; i--) {
        printf("%d ", i);
    }
    printf("\n");

    return 0;
}
