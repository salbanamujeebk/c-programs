#include <stdio.h>

int main()
{
    int num,sum;
    printf("Enter a number : ");
    scanf("%d",&num);
    for(int i=1;i<=num/2;i++)
    {
        if(num%i==0)
        {
            sum=sum+i;
        }

    }

    if(sum==num)
    {
        printf("True!");
    }
    else
    {
        printf("False!");
    }
    return 0;
}
